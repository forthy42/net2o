#! /usr/local/bin/gforth-fast
\ -*-Forth-*-

require net2o.fs

: id-export ( addr u -- )
    ." Passphrase: " +passphrase cr key>default read-keys
    2dup nick-key >o pack-pubkey o>
    [: type ." .n2o" ;] $tmp w/o create-file throw
    >r keypack-buf cmdbuf# @ r@ write-file throw r> close-file throw ;

next-arg id-export bye
