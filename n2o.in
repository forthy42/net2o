#! /usr/bin/env gforth-fast
\ -*- mode: Forth -*-

fpath+ @DATADIR@/gforth/@GFVER@/net2o-@PACKAGE_VERSION@

@HAS_GPSD@ [IF] s" unix/gps.fs" open-fpath-file
    0= [IF] 2drop close-file throw : has-gpsd? ; [THEN]
[THEN]

require n2o.fs

script? [IF] cmd-args ++debug %droprate \ read in all debugging stuff
    argc @ 1 > [IF] next-cmd [ELSE] n2o:help [THEN]
    n2o:bye [THEN]
